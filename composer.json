{
    "name": "phpmentors/workflower-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for Workflower",
    "keywords": ["flow", "workflow", "process", "bpm", "bpmn", "bpms", "symfony"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "phpmentors/workflower": "dev-feature/add-symfony-5-support",
        "phpmentors/stagehand-fsm": "dev-feature/symfony-5-support as 3.0@dev",
        "symfony/config": "^4.3|^5.0",
        "symfony/dependency-injection": "^4.3|^5.0",
        "symfony/finder": "^4.3|^5.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/security-core": "^4.3|^5.0",
        "symfony/security-bundle": "^4.3|^5.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "symfony/doctrine-bridge": "^4.3|^5.0"
    },
    "suggest": {
        "doctrine/orm": ">= 2.4.0 provides transparent serialization/deserialization support for entities with Doctrine ORM",
        "symfony/doctrine-bridge": ">=4.3 provides transparent serialization/deserialization support for entities with Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\WorkflowerBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:alexeevdv/workflower.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:alexeevdv/stagehand-fsm.git"
        }
    ]
}
